{
  "name": "credit-system-sdk",
  "version": "1.0.0",
  "description": "A JavaScript/TypeScript SDK for credit system management - compatible with React, Angular, Vue and any JS framework",
  "main": "./index.js",
  "module": "./esm/index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./esm/index.js",
      "require": "./index.js",
      "default": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "index.js",
    "index.js.map",
    "index.d.ts",
    "index.d.ts.map",
    "api/**/*",
    "auth/**/*",
    "core/**/*",
    "esm/**/*",
    "types/**/*",
    "utils/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build:check",
    "build:check": "node -e \"console.log('Build verified')\"",
    "pack:local": "npm pack",
    "build:bundle": "webpack --config webpack.bundle.config.js",
    "build:bundle:dev": "webpack --config webpack.bundle.config.js --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/developersupreme/credit-system-sdk.git"
  },
  "keywords": [
    "credit",
    "system",
    "sdk",
    "api",
    "client",
    "react",
    "angular",
    "vue",
    "javascript",
    "typescript",
    "browser",
    "node"
  ],
  "author": "developersupreme",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/developersupreme/credit-system-sdk/issues"
  },
  "homepage": "https://github.com/developersupreme/credit-system-sdk#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "peerDependencies": {
    "angular": ">=2.0.0",
    "react": ">=16.8.0",
    "vue": ">=2.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "angular": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "babel-loader": "^10.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  }
}
